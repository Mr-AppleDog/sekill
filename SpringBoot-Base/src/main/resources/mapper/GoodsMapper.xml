<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.base.springbootbase.mapper.GoodsMapper">

    <select id="list" resultType="com.base.springbootbase.domain.entity.Goods">
        select id,goods_name,goods_title,goods_img,goods_detail,goods_price,goods_stock
        from t_goods
    </select>

</mapper>
